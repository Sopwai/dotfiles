! special
*.foreground:   #C5A7E2
*.background:   #221124
*.cursorColor:  #C5A7E2

! black
*.color0:       #221124
*.color8:       #C5A7E2

! red
*.color1:       #A14D64
*.color9:       #C5A7E2

! green
*.color2:       #52368A
*.color10:      #f29372

! yellow
*.color3:       #5D569A
*.color11:      #aeaee6

! blue
*.color4:       #474BBD
*.color12:      #e655bf

! magenta
*.color5:       #AB58A6
*.color13:      #32909b

! cyan
*.color6:       #A08F89
*.color14:      #f63cc9

! white
*.color7:       #C5A7E2
*.color15:      #3c95a0

! terminal transparency
URxvt*transparent: true
URxvt*shading: 3

! common extensions 
URxvt.perl-ext-common: default,matcher,resize-font,tabbed
URxvt.tabbed.tabbar-fg: 3
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 0
URxvt.tabbed.tab-bg: 3


! no scrollbar
URxvt.scrollBar: false

! clickable urls
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

! font
URxvt.font: xft:GohuFont-Medium:size=14
